{
  "name": "server",
  "version": "1.1.2",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "prisma": "prisma",
    "seed": "tsx prisma/seed.ts",
    "update:version": "tsx scripts/update-version.ts",
    "import:pdf": "tsx scripts/import-pdf.ts",
    "import:kjv": "tsx scripts/import-kjv.ts",
    "test:db:create": "createdb chapteraday_test || true",
    "test:db:migrate": "TEST_DATABASE_URL=\"postgresql://$(whoami)@localhost:5432/chapteraday_test?schema=public\" npx prisma migrate deploy",
    "test:db:reset": "dropdb chapteraday_test --if-exists && createdb chapteraday_test && npm run test:db:migrate",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:setup": "npm run test:db:create && npm run test:db:migrate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.19.2",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/supertest": "^6.0.3",
    "jest": "^29.7.0",
    "pdf-parse": "^2.4.5",
    "prisma": "^6.19.2",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
